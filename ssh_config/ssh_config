Host vps
    HostName 5.181.5.168
    User trader
    IdentityFile ~/.ssh/vps_key
    Port 22
    ServerAliveInterval 60
    ServerAliveCountMax 3

# Alternative connection method with full path
Host vps-fullpath
    HostName 5.181.5.168
    User trader
    IdentityFile %USERPROFILE%\.ssh\vps_key
    Port 22
    ServerAliveInterval 60
    ServerAliveCountMax 3
